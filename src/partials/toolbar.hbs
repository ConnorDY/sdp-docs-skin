<div class="toolbar" role="navigation">
{{> nav-toggle}}
  {{#if site.homeUrl}}
  <a href="{{relativize site.homeUrl}}" class="home-link{{#if page.home}} is-current{{/if}}"></a>
  {{/if}}
{{> breadcrumbs}}
  {{#if (and page.fileUri (not env.CI))}}
  <a class='edit-this-page' href="{{page.fileUri}}">Edit on GitHub</a>
  {{else if (and page.editUrl (not page.origin.private))}}
  <a class='edit-this-page' href="{{page.editUrl}}">Edit on GitHub</a>
  {{/if}}
</div>
  <div class="progress-container">
    <div class="progress-bar" id="myBar"></div>
  </div>
